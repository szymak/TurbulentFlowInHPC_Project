<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <flow Re="2500" />
    <simulation finalTime="1" >
      <type>turbulence</type>
      <boundary_layer>turbulent</boundary_layer>
        <scenario>channel</scenario>
    </simulation>
    <backwardFacingStep xRatio="0" yRatio="0" />
    <timestep dt="1" tau="0.5" />
    <solver gamma="0.5" />
    <geometry
      dim="2"
      lengthX="12.0" lengthY=".1" lengthZ=".1"
      sizeX="25" sizeY="41" sizeZ="1"
      stretchX="false" stretchY="true" stretchZ="true"
    >
      <mesh>stretched</mesh>
      <!--mesh>stretched</mesh-->
    </geometry>
    <environment gx="0" gy="0" gz="0" />
    <walls>
        <left>
            <vector x="1.0" y="0" z="0" />
        </left>
        <right>
            <vector x="0" y="0" z="0" />
        </right>
        <top>
            <vector x="0.0" y="0." z="0." />
        </top>
        <bottom>
            <vector x="0" y="0" z="0" />
        </bottom>
        <front>
            <vector x="0" y="0" z="0" />
        </front>
        <back>
            <vector x="0" y="0" z="0" />
        </back>
    </walls>
    <vtk interval="0.01">channel_result_Re2500_TurbulentBL</vtk>
    <stdOut interval="0.0001" />
    <parallel numProcessorsX="1" numProcessorsY="1" numProcessorsZ="1" />
</configuration>
